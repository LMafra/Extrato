<p id="notice"><%= notice %></p>

<h1>Listing Indicator Federations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @indicator_federations.each do |indicator_federation| %>
      <tr>
        <td><%= indicator_federation.name %></td>
        <td><%= indicator_federation.value %></td>
        <td><%= link_to 'Show', indicator_federation %></td>
        <td><%= link_to 'Edit', edit_indicator_federation_path(indicator_federation) %></td>
        <td><%= link_to 'Destroy', indicator_federation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Indicator federation', new_indicator_federation_path %>
